n      <v-expansion-panel-header><h4>Results</h4></v-expansion-panel-header>\n      <v-expansion-panel-content>\n        <div>\n          <v-data-table\n            :class=\"table\"\n            :headers=\"headers\"\n            :items=\"tools\"\n            :items-per-page=\"10\"\n            :search=\"search\"\n            :sort-by=\"['score']\"\n            :sort-desc=\"[true]\"\n            :single-expand=\"singleExpand\"\n            :expanded.sync=\"expanded\"\n            item-key=\"Tool\"\n            show-expand\n            multi-sort\n            class=\"elevation-1\"\n          >\n            <template v-slot:top>\n              <v-toolbar flat>\n                <v-text-field\n                    v-model=\"search\"\n                    append-icon=\"mdi-magnify\"\n                    label=\"Search\"\n                    single-line\n                ></v-text-field>\n                <v-spacer></v-spacer>\n                  <v-switch\n                        v-model=\"singleExpand\"\n                        label=\"Single expand\"\n                        class=\"mt-2\"\n                  ></v-switch>\n                </v-toolbar>\n            </template>\n            <template v-slot:expanded-item=\"{ headers, item }\">\n              <td :colspan=\"headers.length\">\n                {{ item.Description }}\n              </td>\n            </template>\n            <template v-slot:[`item.Tool`]=\"{ item }\">\n              <a :href=\"item.URL\">{{ item.Tool }}</a>\n            </template>\n            <template v-slot:[`item.Description`]=\"{ item }\">\n              {{ trimDescription(item.Description) }}\n            </template>\n          </v-data-table>\n        </div>\n      </v-expansion-panel-content>\n    </v-expansion-panel>\n  </v-expansion-panels>\n</div>\n</template>\n\n<style>\n.table >>> th {\n  font-size: smaller !important; \n}\n.table >>> td {\n  font-size: 10rem !important; \n}\n</style>\n\n<script>\nexport default {\n  name: 'Results',\n  props: ['tools', 'inputParameters'],\n  data () {\n    return {\n      activeResults: true,\n      panel: 0,\n      search: '',\n      expanded: [],\n      singleExpand: false,\n      headers: [\n        { text: '', value: 'data-table-expand' },\n        {text: 'Tool', align: 'start', sortable: false, value: 'Tool'},\n        {text: 'Description', value: 'Description', width: '25rem'},\n        {text: 'Related Topics', value: 'Topics'},\n        {text: 'Functionality', value: 'Operations'},\n        {text: 'Type of Software', value: 'Type'},\n        {text: 'Number of Citations', value: 'Citations'},\n        {text: 'Homepage', value: 'URL'},\n        {text: 'License', value: 'License'},\n        {text: 'Score', value: 'keywords_score'}\n      ],\n      inputsHeaders: [\n        {text: 'Keyword', value: 'keyword'},\n        {text: 'Weight', value: 'weight'}\n      ]\n    }\n  },\n  methods:{\n    trimDescription(desc){\n      var short_desc = `${desc.substring(0,100)}...`\n      return(short_desc)\n    }\n  }\n}\n</script>\n"]}]}