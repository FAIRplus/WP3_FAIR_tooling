     standoff: 8,\n                  font: {\n                    size: 8\n                  }},\n              tickformat: 'd',\n              automargin: true\n              },\n              xaxis : {\n                title: {\n                  text: 'year',\n                  standoff: 5,\n                  font: {\n                    size: 8\n                  }\n                },\n                tickangle: 30,\n                tickformat: 'd',\n                automargin: true\n                }};\n\nexport default {\n  name: 'PubPlot',\n  props: ['pubPlotProps'],\n  data() {\n    return {\n      labels : [],\n      plot: true\n    }\n  },\n  mounted() {\n    if(this.pubPlotProps.citations.length === 0){\n      this.plot = false\n      }\n    else{this.plot = true}\n    Plotly.newPlot(this.pubPlotProps._id, /* JSON object */ {\n      \"data\": this.build_traces(),\n      \"config\": { \"displayModeBar\": false  },\n      \"layout\": layout\n      })\n  },\n  methods: {\n    build_traces(){\n      var traces = []\n      for (let i = 0; i < this.pubPlotProps.citations.length; i++) {\n        var pub = this.pubPlotProps.citations[i]\n        var new_data = {x : pub['trace']['x'], \n                        y : pub['trace']['y'],\n                        name: pub['title'],\n                        type: 'scatter',\n                        text: Array(pub['trace']['x'].length).fill(pub['title']),\n                        hovertemplate: \n                          \"year: %{x:d} </br></br>\" + \n                          \"count: %{y:d}\" +\n                          \"<extra></extra>\",\n                        mode: 'lines+markers',\n                        line: {\n                          width: 1\n                        },\n                        marker:{\n                          size: 4\n                        }\n                        }\n        this.labels.push(pub['title'])\n        traces.push(new_data)\n      }\n      return(traces)\n    }\n}\n}\n</script>\n"]}]}